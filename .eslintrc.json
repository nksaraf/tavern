{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": ["error", "always"],
        "no-else-return": 0,
        "no-await-in-loop": 0,
        "no-underscore-dangle": 0,
        "no-restricted-syntax": 0,
        "comma-dangle": 0,
        "no-unused-vars": 1
    }
}